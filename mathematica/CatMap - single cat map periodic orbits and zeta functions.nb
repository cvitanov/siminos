Cell["\<\
Conversions between number of fixed points and prime periodic orbits (mainly \
used to check the validity of fixed point numbers):\
\>", "Text",
 CellChangeTimes->{{3.671834274810644*^9, 3.671834317304227*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"PToF", "[", "ps_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"fs", "=", 
        RowBox[{"{", 
         RowBox[{"ps", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ",", "i", ",", "ids"}], 
      "}"}], ",", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"ids", "=", 
          RowBox[{
           RowBox[{"Divisors", "[", "i", "]"}], "//", "Most"}]}], ";", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"fs", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"ps", "[", 
              RowBox[{"[", "i", "]"}], "]"}], " ", "i"}], "+", 
            RowBox[{
             RowBox[{"ps", "[", 
              RowBox[{"[", "ids", "]"}], "]"}], ".", "ids"}]}]}], "]"}]}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "2", ",", 
          RowBox[{"Length", "[", "ps", "]"}]}], "}"}]}], "]"}], ";", "fs"}]}],
     "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FToP", "[", "fs_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ps", "=", 
        RowBox[{"{", 
         RowBox[{"fs", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ",", "i", ",", "ids"}], 
      "}"}], ",", 
     RowBox[{
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"ids", "=", 
          RowBox[{
           RowBox[{"Divisors", "[", "i", "]"}], "//", "Most"}]}], ";", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"ps", ",", 
           FractionBox[
            RowBox[{
             RowBox[{"fs", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "-", 
             RowBox[{
              RowBox[{"ps", "[", 
               RowBox[{"[", "ids", "]"}], "]"}], ".", "ids"}]}], "i"]}], 
          "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "2", ",", 
          RowBox[{"Length", "[", "fs", "]"}]}], "}"}]}], "]"}], ";", "ps"}]}],
     "]"}]}], ";"}]}], "Input"]
}, Open  ]]
